単語一覧
<table border="1">
  <thead>
  <tr>
    <th></th>
    <th>英語</th>
    <th>日本語</th>
  </tr>
  </thead>
  <tbody>
  <% @words.each do |word| %>
      <tr>
        <td><%= link_to("詳細", "/words/#{word.id}") %></td>
        <td><%= word.english %></td>
        <td><%= word.japanese %></td>
      </tr>
  <% end %>
  <%= paginate @words %>
  </tbody>
</table>